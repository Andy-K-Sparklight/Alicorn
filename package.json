{
  "private": true,
  "appVersion": "Pre1",
  "version": "1.0.0",
  "sideEffects": false,
  "scripts": {
    "lc": "yarn lcd",
    "df": "yarn dev-full",
    "wd": "yarn watch-dev",
    "lcd": "yarn launch-dev",
    "lcr": "yarn launch-release",
    "build-vercel": "yarn release-full",
    "release-full": "npx webpack --config webpack.config.prod.js --progress",
    "watch-release": "npx webpack --config webpack.config.prod.js --progress --watch",
    "dev-full": "npx webpack --config webpack.config.dev.js --progress",
    "watch-dev": "npx webpack --config webpack.config.dev.js --progress --watch",
    "compress": "node pack.js",
    "windows": "yarn electron-packager dist/release Alicorn --overwrite --platform=win32 --arch=x64 --icon ./resources/build/icon --out out&&yarn electron-packager dist/release Alicorn --overwrite --platform=win32 --arch=ia32 --icon ./resources/build/icon --out out",
    "macos": "yarn electron-packager dist/release Alicorn --overwrite --platform=darwin --arch=x64 --icon ./resources/build/icon --out out",
    "linux": "yarn electron-packager dist/release Alicorn --overwrite --platform=linux --arch=x64 --icon ./resources/build/icon --out out&&node makesh.js",
    "clean": "node clean.js",
    "launch-dev": "cd dist&&npx electron .",
    "launch-release": "cd dist&&cd release&&npx electron .",
    "make": "yarn clean&&yarn release-full&&yarn windows&&yarn macos&&yarn linux&&yarn compress",
    "github-unix": "yarn clean&&yarn release-full&&yarn macos&&yarn linux&&yarn compress",
    "github-windows": "yarn clean&&yarn release-full&&yarn windows&&yarn compress"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.2",
    "@types/fs-extra": "^9.0.12",
    "@types/js-levenshtein": "^1.1.0",
    "@types/object-hash": "^2.1.1",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/semver": "^7.3.8",
    "@types/sha": "^3.0.1",
    "@types/tunnel": "^0.0.3",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "copy-webpack-plugin": "^9.0.1",
    "electron": "^13.1.9",
    "electron-packager": "^15.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "yarn": "^1.22.11"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "ajv": "^8.6.2",
    "cheerio": "^1.0.0-rc.10",
    "compressing": "^1.5.1",
    "copy-to-clipboard": "^3.3.1",
    "crypto-js": "^4.1.1",
    "fs-extra": "^10.0.0",
    "got": "^11.8.2",
    "is-reachable": "^5.0.0",
    "js-base64": "^3.6.1",
    "js-levenshtein": "^1.1.6",
    "mdiff": "^1.3.1",
    "node-machine-id": "^1.1.12",
    "object-hash": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "semver": "^7.3.5",
    "sha": "^3.0.0",
    "toml": "^3.0.0",
    "tunnel": "^0.0.6",
    "uuid": "^8.3.2",
    "ws": "^8.1.0"
  }
}
