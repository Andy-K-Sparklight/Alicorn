  "version": "2.0.0",
  "updatorVersion": 52,
  "scripts": {
    "dev": "concurrently \"pnpm serve\" \"pnpm wd\"",
    "serve": "webpack serve --config webpack.config.dev.js",
    "launch-serve": "cd dist && cross-env ALICORN_DEV_SERVER=http://localhost:9000/Renderer.html electron .",
    "lc": "pnpm lcd",
    "df": "pnpm dev-full",
    "wd": "pnpm watch-dev",
    "lcd": "pnpm launch-dev",
    "lcr": "pnpm launch-release",
    "wd2": "pnpm watch-dev & pnpm devtools",
    "make-patch": "pnpm release-full&&node makepatch.js",
    "release-full": "pnpm rmx&&npx webpack --config webpack.config.prod.js --progress",
    "watch-release": "pnpm rmx&&npx webpack --config webpack.config.prod.js --progress --watch",
    "dev-full": "pnpm rmx&&npx webpack --config webpack.config.dev.js --progress",
    "devtools": "npx react-devtools",
    "watch-dev": "pnpm rmx&&npx webpack --config webpack.config.dev.js --progress --watch",
    "compress": "node eliminate.js&&node pack.js",
    "windows": "pnpm electron-packager dist/release Alicorn --overwrite --platform=win32 --arch=x64 --icon ./resources/build/icon --out out&&pnpm electron-packager dist/release Alicorn --overwrite --platform=win32 --arch=ia32 --icon ./resources/build/icon --out out",
    "macos": "pnpm electron-packager dist/release Alicorn --overwrite --platform=darwin --arch=x64 --icon ./resources/build/icon --out out",
    "linux": "pnpm electron-packager dist/release Alicorn --overwrite --platform=linux --arch=x64 --icon ./resources/build/icon --out out&&pnpm electron-packager dist/release Alicorn --overwrite --platform=linux --arch=arm64 --icon ./resources/build/icon --out out&&node makesh.js",
    "clean": "node clean.js",
    "launch-dev": "cd dist&&npx electron .",
    "launch-release": "cd dist&&cd release&&npx electron .",
    "make": "pnpm clean&&pnpm release-full&&pnpm windows&&pnpm linux&&pnpm compress",
    "github-unix": "pnpm rmx&&pnpm clean&&pnpm release-full&&pnpm linux&&pnpm compress",
    "github-windows": "pnpm rmx&&pnpm clean&&pnpm release-full&&pnpm windows&&pnpm compress",
    "rmx": "rm -rf node_modules/@types/react-router/node_modules node_modules/@types/react-transition-group/node_modules node_modules/@types/react-dom/node_modules node_modules/@types/react-router-dom/node_modules node_modules/@types/react-is/node_modules"
  },
