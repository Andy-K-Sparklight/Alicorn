{
  "private": true,
  "appVersion": "Sonic Rainboom",
  "version": "1.0.0",
  "updatorVersion": 30,
  "sideEffects": false,
  "scripts": {
    "lc": "yarn lcd",
    "df": "yarn dev-full",
    "wd": "yarn watch-dev",
    "lcd": "yarn launch-dev",
    "lcr": "yarn launch-release",
    "wd2": "yarn watch-dev & yarn devtools",
    "make-patch": "yarn release-full&&node makepatch.js",
    "release-full": "npx webpack --config webpack.config.prod.js --progress",
    "watch-release": "npx webpack --config webpack.config.prod.js --progress --watch",
    "dev-full": "npx webpack --config webpack.config.dev.js --progress",
    "devtools": "npx react-devtools",
    "watch-dev": "npx webpack --config webpack.config.dev.js --progress --watch",
    "compress": "node pack.js",
    "windows": "yarn electron-packager dist/release Alicorn --overwrite --platform=win32 --arch=x64 --icon ./resources/build/icon --out out&&yarn electron-packager dist/release Alicorn --overwrite --platform=win32 --arch=ia32 --icon ./resources/build/icon --out out",
    "macos": "yarn electron-packager dist/release Alicorn --overwrite --platform=darwin --arch=x64 --icon ./resources/build/icon --out out",
    "linux": "yarn electron-packager dist/release Alicorn --overwrite --platform=linux --arch=x64 --icon ./resources/build/icon --out out&&node makesh.js",
    "clean": "node clean.js",
    "launch-dev": "cd dist&&npx electron .",
    "launch-release": "cd dist&&cd release&&npx electron .",
    "make": "yarn clean&&yarn release-full&&yarn windows&&yarn linux&&yarn compress",
    "github-unix": "yarn clean&&yarn release-full&&yarn linux&&yarn compress",
    "github-windows": "yarn clean&&yarn release-full&&yarn windows&&yarn compress",
    "rmx": "rm -rf node_modules/@types/react-router/node_modules node_modules/@types/react-transition-group/node_modules node_modules/@types/react-dom/node_modules node_modules/@types/react-router-dom/node_modules"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/js-levenshtein": "^1.1.0",
    "@types/object-hash": "^2.2.1",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/semver": "^7.3.9",
    "@types/sha": "^3.0.1",
    "@types/throttle-debounce": "^2.1.0",
    "@types/tunnel": "^0.0.3",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "copy-webpack-plugin": "^9.1.0",
    "electron": "^16.0.0",
    "electron-packager": "^15.4.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.4.1",
    "react-devtools": "^4.21.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.63.0",
    "webpack-cli": "^4.9.1",
    "yarn": "^1.22.17"
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.1.0",
    "@mui/material": "^5.1.0",
    "@mui/styles": "^5.1.0",
    "ajv": "^8.8.0",
    "cheerio": "^1.0.0-rc.10",
    "compressing": "^1.5.1",
    "copy-to-clipboard": "^3.3.1",
    "crypto-js": "^4.1.1",
    "fs-extra": "^10.0.0",
    "is-reachable": "^5.1.0",
    "js-base64": "^3.7.2",
    "js-levenshtein": "^1.1.6",
    "mdiff": "^1.3.1",
    "node-machine-id": "^1.1.12",
    "node-stream-zip": "^1.15.0",
    "object-hash": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "semver": "^7.3.5",
    "sha": "^3.0.0",
    "sudo-prompt": "^9.2.1",
    "throttle-debounce": "^3.0.1",
    "toml": "^3.0.0",
    "uuid": "^8.3.2",
    "v8-compile-cache": "^2.3.0",
    "ws": "^8.2.3"
  }
}
