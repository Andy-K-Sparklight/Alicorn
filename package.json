{
  "private": true,
  "appVersion": "Alpha",
  "version": "1.0.0",
  "sideEffects": false,
  "scripts": {
    "build-dll": "npx webpack --config webpack.config.dll.js --progress",
    "build-vercel": "yarn build-dll-ci&&yarn release-full-ci&&yarn build-full-ci",
    "prepare-release": "yarn release-full",
    "prepare-dev": "yarn build-dll&&yarn build-full",
    "build-dll-ci": "npx webpack --config webpack.config.dll.js",
    "build-main": "npx webpack --config webpack.config.main.js --progress",
    "watch-main": "npx webpack --config webpack.config.main.js --progress --watch",
    "build-main-ci": "npx webpack --config webpack.config.main.js",
    "build-renderer": "npx webpack --config webpack.config.renderer.js --progress",
    "watch-renderer": "npx webpack --config webpack.config.renderer.js --progress --watch",
    "build-renderer-ci": "npx webpack --config webpack.config.renderer.js",
    "build-star": "npx webpack --config webpack.config.starlight.js --progress&&node mixin.js",
    "build-star-ci": "npx webpack --config webpack.config.starlight.js&&node mixin.js",
    "build-full": "yarn build-main&&yarn build-renderer&&yarn build-star",
    "build-full-ci": "yarn build-main-ci&&yarn build-renderer-ci&&yarn build-star-ci",
    "quick-test": "cd dist&&npx electron .",
    "full-test": "yarn build-full&&yarn quick-test",
    "main-test": "yarn build-main&&yarn quick-test",
    "renderer-test": "yarn build-renderer&&yarn quick-test",
    "star-test": "yarn build-star&&yarn quick-test",
    "release-main": "npx webpack --config webpack.config.main.release.js --progress",
    "release-main-ci": "npx webpack --config webpack.config.main.release.js",
    "release-renderer": "npx webpack --config webpack.config.renderer.release.js --progress",
    "release-renderer-ci": "npx webpack --config webpack.config.renderer.release.js",
    "release-star": "npx webpack --config webpack.config.starlight.release.js --progress&&node mixin.js",
    "release-star-ci": "npx webpack --config webpack.config.starlight.release.js&&node mixin.js",
    "release-full": "yarn release-main&&yarn release-renderer&&yarn release-star",
    "release-full-ci": "yarn release-main-ci&&yarn release-renderer-ci&&yarn release-star-ci",
    "compress": "node pack.js",
    "windows": "yarn electron-packager dist/release Alicorn --overwrite --platform=win32 --arch=x64 --icon ./resources/build/icon --out out&&yarn electron-packager dist/release Alicorn --overwrite --platform=win32 --arch=ia32 --icon ./resources/build/icon --out out",
    "macos": "yarn electron-packager dist/release Alicorn --overwrite --platform=darwin --arch=x64 --icon ./resources/build/icon --out out",
    "linux": "yarn electron-packager dist/release Alicorn --overwrite --platform=linux --arch=x64 --icon ./resources/build/icon --out out&&node makesh.js",
    "clean": "node clean.js",
    "make": "yarn clean&&yarn prepare-release&&yarn windows&&yarn macos&&yarn linux&&yarn compress",
    "github-unix": "yarn clean&&yarn prepare-release&&yarn macos&&yarn linux&&yarn compress",
    "github-windows": "yarn clean&&yarn prepare-release&&yarn windows&&yarn compress"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.1",
    "@types/fs-extra": "^9.0.11",
    "@types/global-tunnel-ng": "^2.1.0",
    "@types/js-levenshtein": "^1.1.0",
    "@types/object-hash": "^2.1.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/semver": "^7.3.6",
    "@types/sha": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "copy-webpack-plugin": "^9.0.0",
    "electron": "^13.1.2",
    "electron-packager": "^15.2.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.1",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2",
    "yarn": "^1.22.10"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "ajv": "^8.6.0",
    "cheerio": "^1.0.0-rc.10",
    "compressing": "^1.5.1",
    "copy-to-clipboard": "^3.3.1",
    "crypto-js": "^4.0.0",
    "fs-extra": "^10.0.0",
    "global-tunnel-ng": "^2.7.1",
    "got": "^11.8.2",
    "js-base64": "^3.6.1",
    "js-levenshtein": "^1.1.6",
    "mdiff": "^1.3.1",
    "node-machine-id": "^1.1.12",
    "object-hash": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "safe-eval": "^0.4.1",
    "semver": "^7.3.5",
    "sha": "^3.0.0",
    "toml": "^3.0.0",
    "v8-compile-cache": "^2.3.0"
  }
}
